{
  "name": "echanneling-corporate-agent-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:firefox": "cypress run --browser firefox",
    "cypress:run:edge": "cypress run --browser edge",
    "cypress:run:mobile": "cypress run --config viewportWidth=375,viewportHeight=667",
    "cypress:run:tablet": "cypress run --config viewportWidth=768,viewportHeight=1024",
    "test:e2e": "start-server-and-test dev http://localhost:3000 cypress:run",
    "test:e2e:headless": "start-server-and-test dev http://localhost:3000 'cypress run --headless'",
    "test:auth": "cypress run --spec 'cypress/e2e/01-auth-flow.cy.ts'",
    "test:booking": "cypress run --spec 'cypress/e2e/02-booking-flow.cy.ts'",
    "test:payment": "cypress run --spec 'cypress/e2e/03-payment-flow.cy.ts'",
    "test:realtime": "cypress run --spec 'cypress/e2e/04-realtime-features.cy.ts'",
    "test:mobile": "cypress run --spec 'cypress/e2e/05-mobile-pwa.cy.ts'",
    "test:all": "npm run test:auth && npm run test:booking && npm run test:payment && npm run test:realtime && npm run test:mobile",
    "cypress:coverage": "nyc cypress run",
    "cypress:report": "nyc report --reporter=html",
    "db:seed": "npx prisma db seed",
    "db:studio": "npx prisma studio",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "pwa:icons": "node scripts/generatePWAIcons.js"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "@reduxjs/toolkit": "^2.0.1",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^7.0.2",
    "@types/socket.io": "^3.0.2",
    "@types/validator": "^13.15.3",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "date-fns": "^2.30.0",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.2.2",
    "lucide-react": "0.522.0",
    "multer": "^2.0.2",
    "next": "^14.0.0",
    "nodemailer": "^7.0.9",
    "prisma": "^6.17.1",
    "react": "^18.3.1",
    "react-datepicker": "^4.21.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-redux": "^9.0.4",
    "recharts": "^2.15.4",
    "redux-persist": "^6.0.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "ts-node": "^10.9.2",
    "validator": "^13.15.15",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.7",
    "@next/bundle-analyzer": "^14.0.0",
    "@testing-library/cypress": "^10.1.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.18",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "latest",
    "cypress": "^15.5.0",
    "cypress-real-events": "^1.15.0",
    "eslint": "^8.50.0",
    "eslint-config-next": "^14.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nyc": "^17.1.0",
    "postcss": "latest",
    "start-server-and-test": "^2.1.2",
    "tailwindcss": "3.4.17",
    "typescript": "^5.5.4"
  }
}
