  "splash_pages": null,
  "scope": "/",
  "categories": ["medical", "health", "business"],
  "shortcuts": [
    {
      "name": "Book Appointment",
      "short_name": "Book",
      "description": "Book a new appointment",
      "url": "/appointment-booking",
      "icons": [
        {
          "src": "/icons/book-appointment.svg",
          "sizes": "192x192",
          "type": "image/svg+xml"
        }
      ]
    },
    {
      "name": "View Appointments",
      "short_name": "Appointments",
      "description": "View all appointments",
      "url": "/appointments",
      "icons": [
        {
          "src": "/icons/appointments.svg",
          "sizes": "192x192",
          "type": "image/svg+xml"
        }
      ]
    },
    {
      "name": "Dashboard",
      "short_name": "Dashboard",
      "description": "View dashboard",
      "url": "/dashboard",
      "icons": [
        {
          "src": "/icons/dashboard.svg",
          "sizes": "192x192",
          "type": "image/svg+xml"
        }
      ]
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/dashboard.svg",
      "sizes": "1280x720",
      "type": "image/svg+xml",
      "label": "Dashboard View"
    },
    {
      "src": "/screenshots/booking.svg",
      "sizes": "1280x720",
      "type": "image/svg+xml",
      "label": "Appointment Booking"
    },
    {
      "src": "/screenshots/appointments.svg",
      "sizes": "1280x720",
      "type": "image/svg+xml",
      "label": "Appointments List"
    }
  ],
  "prefer_related_applications": false,
  "related_applications": [],
  "protocol_handlers": [],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "display_override": [
    "window-controls-overlay",
    "standalone",
    "minimal-ui",
    "browser"
  ],
  "lang": "en-US",
  "dir": "ltr",
  "iarc_rating_id": "",
  "file_handlers": [],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  }
}
